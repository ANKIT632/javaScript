

const datas=[{name:"ankit"},{name:"kumar"}];
function getdata(){
setTimeout(()=>{
    let val="";
   datas.forEach((data,index)=>{
       val+=data.name+" " ;
      
   } )
    document.write(val);
},1000);
}

function getdatas(){
setTimeout(()=>{

       datas.push({name:"gupta"});
      
},2000);
}

getdatas();
getdata(); // ankit kumar
getdata();  // ankit kumar


